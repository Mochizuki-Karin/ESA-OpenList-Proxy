{
  "name": "openlist-proxy-server",
  "version": 2,
  "functions": [
    {
      "name": "proxy",
      "file": "function.js",
      "methods": ["GET", "OPTIONS"]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "target": "proxy"
    }
  ],
  "env": {
    "ADDRESS": "@openlist_address",
    "TOKEN": "@openlist_token",
    "WORKER_ADDRESS": "@worker_address",
    "DISABLE_SIGN": "false"
  }
}
